<AnchorPane prefHeight="470.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.farad.EdCarController">
   <children>
      <TextField fx:id="idField" layoutX="46.0" layoutY="41.0" promptText="ID" />
