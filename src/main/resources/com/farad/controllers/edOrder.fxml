<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="340.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.farad.controllers.EdOrderController">
   <children>
      <ComboBox fx:id="idCarField" layoutX="209.0" layoutY="63.0" prefHeight="25.0" prefWidth="86.0" promptText="ID Авто" />
      <ComboBox fx:id="idCustomerField" layoutX="27.0" layoutY="14.0" prefHeight="25.0" prefWidth="125.0" promptText="ID Покупателя" />
      <Button defaultButton="true" layoutX="209.0" layoutY="295.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="25.0" prefWidth="86.0" text="Сохранить" />
      <Button cancelButton="true" layoutX="311.0" layoutY="295.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="25.0" prefWidth="79.0" text="Отмена" />
      <ListView fx:id="carsListView" layoutX="14.0" layoutY="50.0" prefHeight="270.0" prefWidth="153.0" />
      <Spinner fx:id="countField" layoutX="299.0" layoutY="63.0" prefHeight="25.0" prefWidth="57.0" />
      <TextField fx:id="priceField" layoutX="237.0" layoutY="255.0" prefHeight="25.0" prefWidth="117.0" promptText="Общая стоимость" />
      <Button layoutX="242.0" layoutY="98.0" mnemonicParsing="false" onAction="#addCar" text="Добавить" />
      <Button fx:id="deleteButton" layoutX="247.0" layoutY="133.0" mnemonicParsing="false" onAction="#deleteCar" text="Удалить" textAlignment="CENTER" />
   </children>
</AnchorPane>
