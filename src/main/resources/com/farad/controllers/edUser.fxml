<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="245.0" prefWidth="215.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.farad.controllers.EdUserController">
   <children>
      <TextField fx:id="usernameField" layoutX="33.0" layoutY="32.0" promptText="Username" />
      <TextField fx:id="passwordField" layoutX="33.0" layoutY="77.0" promptText="Password" />
      <TextField fx:id="roleField" layoutX="33.0" layoutY="122.0" promptText="Role" />
      <Button defaultButton="true" layoutX="29.0" layoutY="204.0" mnemonicParsing="false" onAction="#save" text="Сохранить">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button cancelButton="true" layoutX="123.0" layoutY="204.0" mnemonicParsing="false" onAction="#cancel" text="Отмена">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
